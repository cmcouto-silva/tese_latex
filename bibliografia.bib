	keywords = {Humans, Computational Biology, Computer Simulation, Models, Genetic, Polymorphism, Single Nucleotide, Genome-Wide Association Study, Algorithms, Epistasis, Genetic, Genotype, Arthritis, Rheumatoid, Case-Control Studies, Chromosomes, Human, Pair 6, Diabetes Mellitus, Type 1, Genetic Heterogeneity, Genetic Loci, Linear Models},
	pages = {325--340},
