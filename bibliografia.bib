	pmcid = {PMC4654846},
	keywords = {Obesity, Metabolic syndrome, Indian population, Weight excess},
	pages = {105},
	file = {Full Text:/home/cmcouto-silva/Zotero/storage/J6VH38IV/Soares et al. - 2015 - Prevalence of metabolic syndrome in the Brazilian .pdf:application/pdf},
}

@article{cardoso_prevalencia_2001,
	title = {Preval{\^e}ncia de fatores de risco para doen{\c c}as cardiovasculares na popula{\c c}{\~a}o {Guaran{\'i}}-{Mby{\'a}} do {Estado} do {Rio} de {Janeiro}},
	volume = {17},
	issn = {0102-311X},
	url = {http://www.scielo.br/scielo.php?script=sci_arttext&pid=S0102-311X2001000200009&lng=pt&tlng=pt},
	doi = {10.1590/S0102-311X2001000200009},
	abstract = {A distribui{\c c}{\~a}o desigual das doen{\c c}as cr{\^o}nico-degenerativas tem sido atribu{\'i}da aos diferentes graus de transforma{\c c}{\~a}o social em diversas popula{\c c}{\~o}es. Estudos internacionais em popula{\c c}{\~o}es ind{\'i}genas submetidas a mudan{\c c}as em seus estilos de vida t{\^e}m mostrado preval{\^e}ncias elevadas de hipertens{\~a}o arterial e de outros fatores de risco cardiovasculares. Foi feito um estudo de preval{\^e}ncia desses fatores de risco na popula{\c c}{\~a}o adulta das aldeias ind{\'i}genas Sapukai, Paraty-Mirim e Araponga no Rio de Janeiro. Ap{\'o}s recenseamento, a popula{\c c}{\~a}o teve os dados coletados atrav{\'e}s de entrevista e de avalia{\c c}{\~o}es cl{\'i}nicas e bioqu{\'i}micas. O universo estudado foi de 80 homens e 71 mulheres. As preval{\^e}ncias em toda a amostra foram respectivamente para: hipertens{\~a}o arterial (4,8\%, 2,6\% e 7,4\%); sobrepeso (26,7\%, 19,5\% e 34,8\%) e obesidade (4,8\%, 3,9\% e 5,8\%); altera{\c c}{\~o}es lip{\'i}dicas do colesterol total (2,8\%, 2,7\% e 2,9\%) e dos triglicer{\'i}dios (12,6\%, 9,5\% e 15,9\%). Todas as preval{\^e}ncias foram superiores no sexo feminino e maiores nas idades mais avan{\c c}adas. Os resultados sugerem que a popula{\c c}{\~a}o avaliada encontra-se sob risco intermedi{\'a}rio para as doen{\c c}as cr{\^o}nicas, mostrando que devem ser empreendidos esfor{\c c}os para controlar os fatores de risco.
          , 
            Social change has been involved in the unequal distribution patterns of chronic diseases in several populations. Among Indian communities experiencing life pattern changes, international studies have reported increased prevalence of hypertension and other cardiovascular risk factors. Such increased prevalence was ascertained in a survey conducted in 1999 in selected Guaran{\'i}-Mby{\'a} communities (Sapukai, Paraty-Mirim, and Araponga) in the State of Rio de Janeiro, Brazil. A population census was carried out and interviews and clinical and biochemical evaluations were conducted with 80 men and 71 women. Observed prevalence of selected risk factors in the overall sample, including men and women, was as follows for the three communities: hypertension (4.8\%, 2.6\%, 7.4\%); overweight (26.7\%, 19.5\%, 34.8\%); total cholesterol levels (2.8\%, 2.7\%, 2.9\%), and increased triglyceride levels (12.6\%, 9.5\%, 15.9\%). All prevalence rates were higher among women and at older ages. The results suggest that the Guaran{\'i} communities have a moderate risk of chronic diseases and that measures to reduce these risk factors should be adopted.},
	number = {2},
	urldate = {2020-11-04},
	journal = {Cadernos de Sa{\'u}de P{\'u}blica},
	author = {Cardoso, Andrey M. and Mattos, In{\^e}s E. and Koifman, Rosalina J.},
	month = mar,
	year = {2001},
	pages = {345--354},
	file = {Full Text:/home/cmcouto-silva/Zotero/storage/35VQR3S8/Cardoso et al. - 2001 - Preval{\^e}ncia de fatores de risco para doen{\c c}as cardi.pdf:application/pdf},
}

@article{dillehay_probing_2009,
	title = {Probing deeper into first {American} studies},
	volume = {106},
	issn = {1091-6490},
	doi = {10.1073/pnas.0808424106},
	abstract = {The initial peopling of the Americas has proved one of the most challenging episodes in reconstructing global prehistory, challenging because researchers struggle with the vagaries of early archaeological site preservation, and debates continue over the date and place of human entry, the rapidity and direction of dispersion, and the variety of cultural responses to climatic change during the terminal Pleistocene period. Despite many recent advances in our understanding of these issues, especially in the areas of genetics and new archaeological discoveries, the field continues facing limitations in the sampling and quality of data, the research problems defined, and the epistemologies and theories applied. Theoretical development of first American studies has been uneven, and its contribution to global issues of early human migration has been restricted. This essay discusses what is known and not known about the process of the first peopling of the Americas from the perspective of archaeology, genetics, and bioanthropology. Some approaches to fill voids in data, methods, and the broader conceptualization of the process also are considered.},
	language = {eng},
	number = {4},
