	keywords = {Genetics, Population, Humans, Selection, Genetic, Computational Biology, Haplotypes, Software, Access to Information, Biostatistics, EHH, footprints of selection, Homozygote, iHS, Rsb, XP-EHH},
	pages = {78--90},
}

