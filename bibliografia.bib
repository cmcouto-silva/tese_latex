	keywords = {Humans, Male, Linear Models, Adult, Female, Obesity, Ethnic Groups, Aged, Aged, 80 and over, Analysis of Variance, Anthropometry, Body Composition, Brazil, Indians, South American, Middle Aged, Nutritional Status, Overweight, Population Dynamics, Social Class},
	pages = {564--571},
