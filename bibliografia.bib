@article{capelli_avaliacao_2001,
	title = {Avalia{\c c}{\~a}o do estado nutricional da comunidade ind{\'i}gena {Parkat{\^e}j{\^e}}, {Bom} {Jesus} do {Tocantins}, {Par{\'a}}, {Brasil}},
