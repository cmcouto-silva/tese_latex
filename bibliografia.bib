	keywords = {Humans, Alleles, Computational Biology, Polymorphism, Single Nucleotide, Genome-Wide Association Study, Genetic Markers, Psoriasis},
	pages = {e1003766},
	file = {Full Text:/home/cmcouto-silva/Zotero/storage/JPARVDCQ/Climer et al. - 2014 - Allele-specific network reveals combinatorial inte.pdf:application/pdf;Full Text:/home/cmcouto-silva/Zotero/storage/M78V27GU/Climer et al. - 2014 - Allele-specific network reveals combinatorial inte.pdf:application/pdf},
