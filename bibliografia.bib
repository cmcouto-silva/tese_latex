	pmid = {23625889},
	keywords = {Genome, Human, Humans, Selection, Genetic, polygenic selection, Multifactorial Inheritance, Mutation, Adaptation, Biological, Polymorphism, Single Nucleotide, Linkage Disequilibrium, adaptation, epistasis, Epistasis, Genetic, Genotype, Metabolic Networks and Pathways, Host-Pathogen Interactions, human evolution, Models, Theoretical, pathway analysis},
	pages = {1544--1558},
}

