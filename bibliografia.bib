	keywords = {Genome, Human, Humans, Polymorphism, Single Nucleotide, Population, Software, Genetic Linkage},
	pages = {559--575},
	file = {Full Text:/home/cmcouto-silva/Zotero/storage/YD9JNC2Q/Purcell et al. - 2007 - PLINK a tool set for whole-genome association and.pdf:application/pdf},
