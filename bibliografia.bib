	pages = {610--621},
	file = {Accepted Version:/home/cmcouto-silva/Zotero/storage/6NI7MNHR/Climer et al. - 2014 - A custom correlation coefficient (CCC) approach fo.pdf:application/pdf;Accepted Version:/home/cmcouto-silva/Zotero/storage/MRQBBTBK/Climer et al. - 2014 - A custom correlation coefficient (CCC) approach fo.pdf:application/pdf},
